    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.6",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dev-utils": "^9.0.11",
